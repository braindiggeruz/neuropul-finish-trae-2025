{
  "name": "neuropul-trae-ai",
  "version": "3.1.0",
  "description": "AI-powered personal growth companion with archetype-based coaching",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite --host",
    "dev:mock-tg": "cross-env VITE_TG_WEBAPP_MOCK=1 vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:a11y": "playwright test tests/a11y/",
    "test:accessibility": "pa11y-ci --sitemap http://localhost:5173/sitemap.xml --threshold 10",
    "test:lighthouse": "lhci autorun || true",
    "test:lighthouse:median": "node scripts/lighthouse-median.mjs",
    "test:contract": "tsx tests/contract/validate.ts",
    "test:size-limit": "size-limit",
    "test:all": "npm run test && npm run test:e2e && npm run test:a11y && npm run test:lighthouse && npm run test:size-limit",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer dist/static/js/*.js",
    "release:assets": "node scripts/generate-release-assets.js",
    "release:prepare": "npm run build && npm run test:all && npm run release:assets",
    "release:deploy": "npm run release:prepare && npm run deploy",
    "postbuild": "echo 'Build complete'"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@supabase/supabase-js": "^2.38.4",
    "@tanstack/react-query": "^5.8.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.292.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "workbox-window": "^7.0.0",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.2",
    "@lhci/cli": "^0.13.0",
    "@playwright/test": "^1.39.0",
    "@size-limit/preset-big-lib": "^11.0.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "axe-playwright": "^2.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^22.1.0",
    "pa11y-ci": "^3.0.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^6.0.4",
    "size-limit": "^11.0.1",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.19.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-pwa": "^0.16.4",
    "vitest": "^0.34.6",
    "web-vitals": "^3.5.0",
    "workbox-build": "^7.0.0",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "size-limit": [
    {
      "path": "dist/assets/*.js",
      "limit": "250 kB"
    }
  ]
}
